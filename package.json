{
  "name": "producktivity",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "FORCE_COLOR=2 turbo run build",
    "dev": "FORCE_COLOR=2 turbo run dev --parallel",
    "lint": "FORCE_COLOR=2 turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "prettier": "^2.7.1",
    "turbo": "^1.5.6"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "packageManager": "pnpm@7.13.5"
}