{
  "name": "producktivity",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "build": "FORCE_COLOR=2 turbo run build",
    "dev": "FORCE_COLOR=2 turbo run dev --parallel",
    "lint": "FORCE_COLOR=2 turbo run lint",
    "format": "FORCE_COLOR=2 turbo run format",
    "build:server": "pnpm turbo run --filter=\"@producktivity/server\" build",
    "start:server": "pnpm --filter=\"@producktivity/server\" start",
    "build:slide": "pnpm turbo run --filter=\"@producktivity/slide\" build"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "crlf-phobia": "^1.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "prisma-nestjs-graphql": "^17.0.1",
    "turbo": "^1.6.1"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "packageManager": "pnpm@7.13.5"
}