{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_GRAPHQL_ENDPOINT",
    "DATABASE_URL",
    "JWT_SECRET",
    "npm_lifecycle_event"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}